			"name": "0. Setup - Get JWT Token",
			"item": [
				{
					"name": "Register Patient User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    console.log('✅ Patient user registered successfully');",
									"    pm.environment.set('jwt_token', pm.response.json().token);",
									"} else if (pm.response.code === 409) {",
									"    console.log('⚠️ User already exists, proceeding to login');",
									"} else if (pm.response.code === 429) {",
									"    console.log('⚠️ Rate limit reached for registration, use existing user or wait');",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
													"mode": "raw",
													"raw": "{\n    \"email\": \"test.patient@example.com\",\n    \"password\": \"password123\",\n    \"full_name\": \"Test Patient\",\n    \"phone_number\": \"9876543210\",\n    \"role\": \"Patient\"\n}"
												},
						"url": {
							"raw": "{{node_url}}/api/auth/register",
							"host": [
								"{{node_url}}"
							],
							"path": [
								"api",
								"auth",
								"register"
							]
						}
					},
					"response": []
				},
				{
					"name": "Login and Get JWT Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set('jwt_token', jsonData.token);",
									"    pm.environment.set('user_id', jsonData.user.userId);",
									"    pm.environment.set('user_uuid', jsonData.user.userUUID);",
									"    console.log('✅ JWT Token saved:', jsonData.token.substring(0, 20) + '...');",
									"    console.log('✅ User ID saved:', jsonData.user.userId);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"test.patient@example.com\",\n    \"password\": \"password123\"\n}"
						},
						"url": {
							"raw": "{{node_url}}/api/auth/login",
							"host": [
								"{{node_url}}"
							],
							"path": [
								"api",
								"auth",
								"login"
							]
						}
					},
					"response": []
				}
			]
		},
